<% if(shownCategory != "") { %>
    <%console.log(shownCategory)%>
    <% shownCategory.forEach(category => { %>
        <div class="single-blog-post featured-post mb-30">
            <div class="post-thumb">
                <a href="/single-post/<%= category.article_id%>"><img src="/img/bg-img/<%= category.img_src%>" alt=""></a>
            </div>
            <div class="post-data"> 
                <a href="#" class="post-catagory"><%= category.categoryName%></a>
                <a href="/single-post/<%= category.article_id%>" class="post-title">
                    <h6><%= category.article_title%></h6>
                </a>
                <div class="post-meta">
                    <p class="post-author">By <a href="#"><%= category.author_name%></a></p>
                    <p class="post-excerp"><%= category.article_summary%></p>
                    <!-- Post Like & Post Comment -->
                    <div class="d-flex align-items-center">
                        <a href="#" class="post-like"><img src="/img/core-img/like.png" alt=""> <span><%= category.article_likes%></span></a>
                        <a href="#" class="post-comment"><img src="/img/core-img/chat.png" alt=""> <span>10</span></a>
                    </div>
                </div>
            </div>
        </div>
    <%}); %>
    <%} else {%>
    <p>Der er ingen artikler</p>
<%}%>